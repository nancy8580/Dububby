{
  "name": "lowcode-crud-backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name auto",
    "publish-model-script": "ts-node src/services/modelPublisher.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-mysql-session": "^2.1.7",
    "mysql2": "^3.6.0",
    "prisma": "^5.12.0",
    "@prisma/client": "^5.12.0",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "cookie": "^0.5.0",
    "cookie-signature": "^1.1.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "@types/express-session": "^1.17.7",
    "vitest": "^1.6.0",
    "@types/node": "^20.8.10",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/cookie": "^0.6.0",
    "@types/cookie-signature": "^1.1.2"
  }
}
